# Client-side environment variables (prefixed with REACT_APP_)
# Required API keys for LLM providers
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Azure OpenAI API settings
REACT_APP_AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
REACT_APP_AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com
REACT_APP_AZURE_OPENAI_API_VERSION=2023-12-01-preview

# Optional: API proxy URL (if you're using a proxy server to avoid CORS issues)
REACT_APP_API_PROXY_URL=/api/proxy

# Optional: Ollama configuration (for local LLM inference)
REACT_APP_OLLAMA_API_URL=http://localhost:11434

# Server configuration (ports for frontend and backend services)
BACKEND_PORT=3002
FRONTEND_PORT=3001 